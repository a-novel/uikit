{
  "extends": "./.svelte-kit/tsconfig.json",
  "exclude": ["src/locales"],
  "compilerOptions": {
    "types": ["@testing-library/jest-dom"],
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "Bundler"
  }
}
