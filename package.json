{
  "name": "@anovel/uikit",
  "version": "0.2.1",
  "homepage": "https://github.com/a-novel/uikit#readme",
  "scripts": {
    "dev": "BROWSER=none PORT=2060 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --no-cache",
    "registry:build": "npx webpack --config webpack.config.js",
    "registry:clean": "rm -rf ./dist",
    "registry:compile": "npm run registry:clean && npm run registry:build",
    "registry:publish": "npm run registry:compile && npm publish --access public"
  },
  "exports": {
    "./components/stateful": {
      "types": "./dist/components/stateful/index.d.ts",
      "default": "./dist/components/stateful.js"
    },
    "./components/stateless": {
      "types": "./dist/components/stateless/index.d.ts",
      "default": "./dist/components/stateless.js"
    },
    "./contexts": {
      "types": "./dist/contexts/index.d.ts",
      "default": "./dist/contexts.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "default": "./dist/hooks.js"
    },
    "./lib/backend": {
      "types": "./dist/lib/backend/index.d.ts",
      "default": "./dist/lib/backend.js"
    },
    "./lib/api": {
      "types": "./dist/lib/api/index.d.ts",
      "default": "./dist/lib/api.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib.js"
    },
    "./public/*": "./public/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "components/stateful": ["dist/components/stateful/index.d.ts"],
      "components/stateless": ["dist/components/stateless/index.d.ts"],
      "contexts": ["dist/contexts/index.d.ts"],
      "hooks": ["dist/hooks/index.d.ts"],
      "lib/backend": ["dist/lib/backend/index.d.ts"],
      "lib/api": ["dist/lib/api/index.d.ts"],
      "lib": ["dist/lib/index.d.ts"]
    }
  },
  "dependencies": {
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@types/node": "18.16.1",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.1",
    "highlight.js": "^11.7.0",
    "i18next": "^22.4.15",
    "next": "13.3.1",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "canvas": "^2.11.2",
    "client-only": "^0.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "server-only": "^0.0.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "whatwg-fetch": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-novel/uikit.git"
  },
  "bugs": {
    "url": "https://github.com/a-novel/uikit/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist",
    "*.d.ts"
  ]
}
