{
  "name": "@a-novel/uikit",
  "version": "0.3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-novel/uikit.git"
  },
  "license": "AGPL-3.0",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "svelte": "./dist/utils/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "svelte": "./dist/auth/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "svelte": "./dist/ui/index.js"
    },
    "./ui/components": {
      "types": "./dist/ui/components/index.d.ts",
      "svelte": "./dist/ui/components/index.js"
    },
    "./ui/components/statusPages": {
      "types": "./dist/ui/components/statusPages/index.d.ts",
      "svelte": "./dist/ui/components/statusPages/index.js"
    },
    "./assets/icons/*": "./src/assets/icons/*",
    "./assets/logos/*": "./src/assets/logos/*"
  },
  "files": [
    "dist",
    "messages",
    "src/assets/icons",
    "src/assets/logos"
  ],
  "scripts": {
    "assets:gen": "node src/scripts/assets.gen.ts",
    "build": "vite build && npm run prepack",
    "build:storybook": "storybook build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:ci": "pnpm generate && pnpm check",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write . --config prettier.config.ts",
    "generate": "pnpm generate:translations",
    "generate:translations": "npx wuchale -c --config wuchale.config.ts",
    "preinstall": "npx only-allow pnpm",
    "lint": "prettier --check . --config prettier.config.ts && eslint .",
    "lint:ci": "pnpm generate && pnpm lint",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "publish:major": "pnpm version major && git push --follow-tags",
    "publish:minor": "pnpm version minor && git push --follow-tags",
    "publish:patch": "pnpm version patch && git push --follow-tags",
    "storybook": "storybook dev -p 6006",
    "test": "pnpm run test:unit",
    "test:ci": "pnpm generate && pnpm test",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@wuchale/svelte": "^0.17.5",
    "@wuchale/vite-plugin": "^0.15.3",
    "esm-env": "^1.2.2",
    "wuchale": "^0.18.5"
  },
  "devDependencies": {
    "@a-novel/nodelib-test": "^1.1.1",
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@iconify-json/grommet-icons": "^1.2.4",
    "@iconify-json/material-symbols": "^1.2.48",
    "@iconify-json/nrk": "^1.2.7",
    "@storybook/addon-a11y": "^10.1.3",
    "@storybook/addon-docs": "^10.1.3",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/addon-themes": "^10.1.3",
    "@storybook/addon-vitest": "^10.1.3",
    "@storybook/sveltekit": "^10.1.3",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/svelte": "^5.2.9",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitest/browser": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-storybook": "^10.1.3",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.7.3",
    "prettier-plugin-packagejson": "^2.5.20",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.15",
    "sharp": "^0.34.5",
    "storybook": "^10.1.3",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "unplugin": "^2.3.11",
    "unplugin-icons": "^22.5.0",
    "vite": "^7.2.6",
    "vitest": "^4.0.15",
    "vitest-browser-svelte": "^2.0.1"
  },
  "peerDependencies": {
    "@a-novel/nodelib-browser": "^1.1.1",
    "@a-novel/service-authentication-rest": "^2.1.2",
    "@sveltejs/kit": "^2.49.0",
    "svelte": "^5.0.0",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.24.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "msw"
    ]
  }
}
