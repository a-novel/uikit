{
  "name": "@a-novel/uikit",
  "version": "0.8.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-novel/uikit.git"
  },
  "license": "AGPL-3.0",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "svelte": "./dist/utils/index.js"
    },
    "./const": {
      "types": "./dist/const/index.d.ts",
      "import": "./dist/const/index.js",
      "svelte": "./dist/const/index.js"
    },
    "./locales": {
      "types": "./dist/locales/index.d.ts",
      "import": "./dist/locales/index.js",
      "svelte": "./dist/locales/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.js",
      "svelte": "./dist/ui/index.js"
    },
    "./storybook": {
      "types": "./dist/storybook/index.d.ts",
      "import": "./dist/storybook/index.js",
      "svelte": "./dist/storybook/index.js"
    },
    "./ui/components": {
      "types": "./dist/ui/components/index.d.ts",
      "import": "./dist/ui/components/index.js",
      "svelte": "./dist/ui/components/index.js"
    },
    "./ui/components/statusPages": {
      "types": "./dist/ui/components/statusPages/index.d.ts",
      "import": "./dist/ui/components/statusPages/index.js",
      "svelte": "./dist/ui/components/statusPages/index.js"
    },
    "./ui/components/nav": {
      "types": "./dist/ui/components/nav/index.d.ts",
      "import": "./dist/ui/components/nav/index.js",
      "svelte": "./dist/ui/components/nav/index.js"
    },
    "./assets/*": "./dist/assets/*",
    "./ui/designSystem.css": "./dist/ui/designSystem.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "assets:gen": "node src/scripts/assets.gen.ts",
    "build": "vite build && npm run prepack",
    "build:storybook": "storybook build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write . --config prettier.config.ts",
    "generate": "pnpm generate:translations",
    "generate:translations": "pnpm generate:translations:sync && pnpm generate:translations:pull",
    "generate:translations:pull": "pnpm tolgee pull --api-key ${TOLGEE_API_KEY}",
    "generate:translations:sync": "pnpm tolgee sync --yes --api-key ${TOLGEE_API_KEY}",
    "preinstall": "npx only-allow pnpm",
    "lint": "prettier --check . --config prettier.config.ts && eslint .",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "publish:major": "pnpm version major && git push --follow-tags",
    "publish:minor": "pnpm version minor && git push --follow-tags",
    "publish:patch": "pnpm version patch && git push --follow-tags",
    "storybook": "storybook dev -p 6006",
    "test": "pnpm run test:unit",
    "test:unit": "vitest run"
  },
  "devDependencies": {
    "@a-novel-kit/nodelib-browser": "^1.3.1",
    "@a-novel-kit/nodelib-config": "^1.3.1",
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@iconify/svelte": "^5.1.0",
    "@storybook/addon-a11y": "^10.1.3",
    "@storybook/addon-docs": "^10.1.3",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/addon-themes": "^10.1.3",
    "@storybook/addon-vitest": "^10.1.3",
    "@storybook/sveltekit": "^10.1.3",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.49.1",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/svelte": "^5.2.9",
    "@tolgee/cli": "^2.14.0",
    "@tolgee/format-icu": "^6.2.7",
    "@tolgee/svelte": "^6.2.7",
    "@tolgee/web": "^6.2.7",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitest/browser": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-storybook": "^10.1.3",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "prettier": "^3.7.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-packagejson": "^2.5.20",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.15",
    "sharp": "^0.34.5",
    "storybook": "^10.1.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vitest": "^4.0.15",
    "vitest-browser-svelte": "^2.0.1",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@a-novel-kit/nodelib-browser": "^1.3.1",
    "@iconify/svelte": "^5.1.0",
    "@sveltejs/kit": "^2.49.1",
    "@tolgee/format-icu": "^6.2.7",
    "@tolgee/svelte": "^6.2.7",
    "@tolgee/web": "^6.2.7",
    "svelte": "^5.0.0",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.25.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "msw"
    ]
  },
  "tolgee": {
    "$schema": "https://docs.tolgee.io/cli-schema.json",
    "projectId": 25969,
    "format": "JSON_ICU",
    "defaultNamespace": "common",
    "patterns": [
      "./src/**/*.ts",
      "./src/**/*.svelte"
    ],
    "push": {
      "filesTemplate": "./src/lib/locales/tr/{languageTag}.{namespace}.{extension}",
      "language": [
        "en",
        "fr"
      ],
      "forceMode": "OVERRIDE",
      "removeOtherKeys": true
    },
    "pull": {
      "path": "./src/lib/locales/tr/",
      "filesTemplate": "{languageTag}.{namespace}.{extension}",
      "emptyDir": true,
      "delimiter": ""
    },
    "sync": {
      "removeUnused": true
    }
  }
}
