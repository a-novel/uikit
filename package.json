{
  "name": "@a-novel/uikit",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-novel/uikit.git"
  },
  "license": "AGPL-3.0",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "svelte": "./dist/utils/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "svelte": "./dist/auth/index.js"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "svelte": "./dist/ui/index.js"
    },
    "./ui/components": {
      "types": "./dist/ui/components/index.d.ts",
      "svelte": "./dist/ui/components/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && npm run prepack",
    "build-storybook": "storybook build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "pnpm run test:unit",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "esm-env": "^1.2.2"
  },
  "devDependencies": {
    "@a-novel/nodelib-test": "^1.0.4",
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/addon-themes": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/sveltekit": "^10.0.8",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/package": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/svelte": "^5.2.9",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitest/browser": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.13",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-storybook": "^10.0.8",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.15",
    "storybook": "^10.0.8",
    "svelte-check": "^4.3.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.13",
    "vitest-browser-svelte": "^2.0.1"
  },
  "peerDependencies": {
    "@a-novel/nodelib-browser": "^1.0.4",
    "@a-novel/service-authentication-rest": "^2.1.2",
    "@sveltejs/kit": "^2.49.0",
    "svelte": "^5.0.0",
    "zod": "^4.1.12"
  },
  "packageManager": "pnpm@10.24.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@a-novel/nodelib-test",
      "@a-novel/nodelib-browser",
      "@a-novel/service-authentication-rest",
      "esbuild"
    ]
  }
}
